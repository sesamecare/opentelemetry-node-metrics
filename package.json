{
  "name": "@sesamecare-oss/opentelemetry-node-metrics",
  "version": "1.0.1",
  "description": "A modernized set of node metrics for OpenTelemetry",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "author": "Developers <developers@sesamecare.com>",
  "license": "MIT",
  "packageManager": "yarn@3.8.7",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "yarn dlx rimraf ./dist",
    "lint": "eslint .",
    "postinstall": "cpconfig",
    "test": "vitest"
  },
  "keywords": [
    "typescript",
    "sesame"
  ],
  "files": [
    "build",
    "src",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sesamecare/opentelemetry-node-metrics.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "yarn dlx pinst --disable"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "config": {
    "cpconfig": "@openapi-typescript-infra/cpconfig"
  },
  "devDependencies": {
    "@openapi-typescript-infra/cpconfig": "^1.1.0",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/github": "^12.0.6",
    "@types/node": "^25.2.3",
    "cpconfig": "^1.4.4",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-metrics": "^2.5.1"
  }
}
